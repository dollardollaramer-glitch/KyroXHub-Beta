local function RUMJRS_fake_script() -- sUNC.LocalScript 
	local script = Instance.new('LocalScript', sUNC)

	sUNC.MouseButton1Down:connect(function()
	
		-- sUNC Estimation Script
		-- Estimates executor compatibility based on API availability
	
		local tests = {
			-- Core
			{"getgenv", getgenv},
			{"getrenv", getrenv},
			{"getrawmetatable", getrawmetatable},
			{"setreadonly", setreadonly},
			{"isreadonly", isreadonly},
			{"getnamecallmethod", getnamecallmethod},
			{"setnamecallmethod", setnamecallmethod},
	
			-- Debug
			{"debug.getupvalue", debug and debug.getupvalue},
			{"debug.setupvalue", debug and debug.setupvalue},
			{"debug.getconstants", debug and debug.getconstants},
			{"debug.setconstant", debug and debug.setconstant},
	
			-- Closures / hooks
			{"hookfunction", hookfunction},
			{"newcclosure", newcclosure},
			{"islclosure", islclosure},
			{"iscclosure", iscclosure},
	
			-- Script / instance
			{"getinstances", getinstances},
			{"getscripts", getscripts},
			{"getloadedmodules", getloadedmodules},
	
			-- Filesystem
			{"readfile", readfile},
			{"writefile", writefile},
			{"appendfile", appendfile},
			{"makefolder", makefolder},
			{"listfiles", listfiles},
			{"isfile", isfile},
			{"isfolder", isfolder},
	
			-- Network
			{"request", request or http_request or syn and syn.request},
	
			-- Misc
			{"setclipboard", setclipboard},
			{"queue_on_teleport", queue_on_teleport},
			{"identifyexecutor", identifyexecutor},
		}
	
		local supported = 0
		local total = #tests
		local missing = {}
	
		for _, test in ipairs(tests) do
			local name, func = test[1], test[2]
			if typeof(func) == "function" or func ~= nil then
				supported += 1
			else
				table.insert(missing, name)
			end
		end
	
		local sUNC = math.floor((supported / total) * 100)
	
		print("====== sUNC Report ======")
		print("Executor:", identifyexecutor and identifyexecutor() or "Unknown")
		print("Supported APIs:", supported .. "/" .. total)
		print("Estimated sUNC:", sUNC .. "%")
	
		if #missing > 0 then
			print("Missing APIs:")
			for _, v in ipairs(missing) do
				print(" -", v)
			end
		end
	end)
	
end
coroutine.wrap(RUMJRS_fake_script)()
local function SIJPI_fake_script() -- ExecutorDetector.LocalScript 
	local script = Instance.new('LocalScript', ExecutorDetector)

	ExecutorDetector.MouseButton1Down:connect(function()
		-- sUNC Executor Detection Script
	
		local executor = "Unknown"
	
		local checks = {
			["Synapse X"] = function()
				return syn and syn.request
			end,
	
			["Krnl"] = function()
				return KRNL_LOADED or iskrnlclosure
			end,
	
			["Script-Ware"] = function()
				return identifyexecutor and identifyexecutor():lower():find("script-ware")
			end,
	
			["Fluxus"] = function()
				return fluxus or getexecutorname and getexecutorname():lower():find("fluxus")
			end,
	
			["Electron"] = function()
				return electron
			end,
	
			["Oxygen U"] = function()
				return oxygenu
			end,
	
			["Comet"] = function()
				return comet
			end,
	
			["Evon"] = function()
				return evon
			end,
	
			["Trigon"] = function()
				return trigon
			end,
		}
	
		-- Generic fallback (sUNC-friendly)
		if identifyexecutor then
			executor = identifyexecutor()
		else
			for name, check in pairs(checks) do
				local success, result = pcall(check)
				if success and result then
					executor = name
					break
				end
			end
		end
	
		print("Executor Detected:", executor)
	
	end)
	
end
coroutine.wrap(SIJPI_fake_script)()
