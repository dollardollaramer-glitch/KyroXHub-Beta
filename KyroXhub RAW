-- Scripts:

local function JXCYQ_fake_script() -- InfYield.LocalScript 
	local script = Instance.new('LocalScript', InfYield)

	InfYield.MouseButton1Down:connect(function()
	
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
	end)
	
end
coroutine.wrap(JXCYQ_fake_script)()
local function RWJMK_fake_script() -- prisonlife.LocalScript 
	local script = Instance.new('LocalScript', prisonlife)

	prisonlife.MouseButton1Down:connect(function()
		
		loadstring(game:HttpGet("https://rawscripts.net/raw/MP5-Prison-Life-C00lhub-74755"))()
	end)
	
end
coroutine.wrap(RWJMK_fake_script)()
local function UHJEJA_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	local Close = script.Parent
	local MainFrame = Close.Parent
	
	Close.MouseButton1Down:connect(function()
		MainFrame.Visible = false
	end)
end
coroutine.wrap(UHJEJA_fake_script)()
local function COZUGJ_fake_script() -- SanderXY.LocalScript 
	local script = Instance.new('LocalScript', SanderXY)

	SanderXY.MouseButton1Down:connect(function()
		
		loadstring(game:HttpGet("https://raw.githubusercontent.com/kigredns/testUIDK/refs/heads/main/panel.lua"))()
	end)
	
end
coroutine.wrap(COZUGJ_fake_script)()
