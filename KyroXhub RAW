local function ZBAVZF_fake_script() -- Hi.LocalScript 
	local script = Instance.new('LocalScript', Hi)

	local Players = game:GetService("Players")
	local player = Players.LocalPlayer
	
	-- wait for gui
	local gui = player:WaitForChild("PlayerGui"):WaitForChild("KyroX Hub")
	
	-- keep trying until we get the REAL label
	task.spawn(function()
		while true do
			local hi = gui:FindFirstChild("Hi", true)
			if hi and hi:IsA("TextLabel") then
				hi.Text = "Welcome, " .. player.Name
				hi:GetPropertyChangedSignal("Text"):Connect(function()
					hi.Text = "Welcome, " .. player.Name
				end)
				break
			end
			task.wait(0.1)
		end
	end)
	
	
	
	
	
end
coroutine.wrap(ZBAVZF_fake_script)()
