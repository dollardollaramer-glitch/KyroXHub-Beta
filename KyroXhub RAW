-- Scripts:

local function LKFF_fake_script() -- Scripts.LocalScript 
	local script = Instance.new('LocalScript', Scripts)

	local ScriptsButton = script.Parent
	local ScriptsFrame = ScriptsButton.Parent.Parent.ScriptsFrame
	
	ScriptsButton.MouseButton1Down:connect(function()
		ScriptsFrame.Visible = true
	end)
end
coroutine.wrap(LKFF_fake_script)()
local function DPRJ_fake_script() -- Close.LocalScript 
	local script = Instance.new('LocalScript', Close)

	local Close = script.Parent
	local MainFrame = Close.Parent
	
	Close.MouseButton1Down:connect(function()
		MainFrame.Visible = false
	end)
end
coroutine.wrap(DPRJ_fake_script)()
local function KJRD_fake_script() -- InfYield.LocalScript 
	local script = Instance.new('LocalScript', InfYield)

	InfYield.MouseButton1Down:connect(function()
	
		loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
	end)
	
end
coroutine.wrap(KJRD_fake_script)()
local function NLXRDEY_fake_script() -- ChatBypasser.LocalScript 
	local script = Instance.new('LocalScript', ChatBypasser)

	ChatBypasser.MouseButton1Down:connect(function()
		
		loadstring(game:HttpGet("https://raw.githubusercontent.com/QuorumHub1/QuorumHub/refs/heads/main/ChatBypasser.txt"))()
	end)
	
end
coroutine.wrap(NLXRDEY_fake_script)()
